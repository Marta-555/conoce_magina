{% extends 'base.html.twig' %}

{% block header %}
<section class="module bg-dark-30 portfolio-page-header" data-background="{{ image }}">
  <div class="container">
      <div class="row">
          <div class="col-sm-6 col-sm-offset-3">
              <h1 class="module-title font-alt">{{ titulo }}</h1>
          </div>
      </div>
  </div>
</section>
{% endblock %}

{% block body %}
<section class="module">
  <div class="container" id="contPrincipal">
    <div class="row">
      <div class="col-sm-12">
        <ul class="filter font-alt" id="filtros">
        </ul>
      </div>
    </div>
    <div>
      <div class="col-sm-12">
        <div class="row">
          <div class="col-sm-4 col-md-4 col-md-offset-8 sidebar">
            <div class="widget">
              <div class="search-box">
                <input id="buscador" class="form-control" type="text" placeholder="Search..."/>
                <button class="search-btn" type="submit"><i class="fa fa-search"></i></button>
              </div>
            </div>
          </div>
        </div>
        <div class="row multi-columns-row post-columns" id="contenedor"></div>
        <div class="pagination font-alt">
          <a href="#"><i class="fa fa-angle-left"></i></a>
          <a class="active" href="#">1</a>
          <a href="#">2</a>
          <a href="#">3</a>
          <a href="#">4</a>
          <a href="#"><i class="fa fa-angle-right"></i></a>
        </div>
      </div>
    </div>
  </div>
</section>
{% endblock %}


{% block javascripts %}
{{ parent() }}

<script src="{{ asset('js/ajax-call.js') }}"></script>
{% endblock %}